{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/ledger-service/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/ledger-service/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/ledger-service",
  "projects": {
    "ledger-service": {
      "type": "application",
      "root": "apps/ledger-service",
      "entryFile": "main",
      "sourceRoot": "apps/ledger-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ledger-service/tsconfig.app.json"
      }
    },
    "ledger-app": {
      "type": "application",
      "root": "apps/ledger-app",
      "entryFile": "main",
      "sourceRoot": "apps/ledger-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ledger-app/tsconfig.app.json"
      }
    },
    "job-consumer-app": {
      "type": "application",
      "root": "apps/job-consumer-app",
      "entryFile": "main",
      "sourceRoot": "apps/job-consumer-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/job-consumer-app/tsconfig.app.json"
      }
    },
    "common-lib": {
      "type": "library",
      "root": "libs/common-lib",
      "entryFile": "index",
      "sourceRoot": "libs/common-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common-lib/tsconfig.lib.json"
      }
    }
  }
}